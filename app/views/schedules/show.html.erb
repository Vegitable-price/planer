<div class="main contents">
  <div class="contents_part">
    <% if user_signed_in? && current_user.id == @schedle.user_id %>
      <ul class="more_list">
        <li>
          <%= link_to '編集', edit_schedule_path(@schedule.id), method: :get %>
        </li>
        <li>
          <%= link_to '削除', schedule_path(@schedule.id), method: :delete %>
        </li>
      </ul>
    <% end %>
